{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts", 
      
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "backend/trpc/**",
          "backend/lib/**"
        ]
      }
    }
  ],
  "routes": [
    { 
      "src": "/", 
      "dest": "/api" 
    },
    {
      "src": "/api/(.*)", 
      "dest": "/api" 
    }
  ]
}